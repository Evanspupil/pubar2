div.search(hide-tabs='true')
    ion-content
        div#search-bar
            div.item-input-inset
                label.item-input-wrapper
                    i.icon.ion-search.placeholder-icon
                    input(type="search" placeholder="请输入城市名" name="" ng-model="form.key" autocorrect="off")
                button.button.button-clear 取消


        div.row
            div.col-33
                div
                    i.icon.ion-wineglass
                    p 酒局
            div.col-33
                div
                    i.icon.ion-ios-rose
                    p 酒吧
            div.col-33
                div
                    i.icon.ion-ios-personadd
                    p 酒友


        div.result
            nav-tip-bar(title='相关的酒吧')
            div.list
                a.item.item-avatar
                    img(ng-src='{{vm.barAvatar}}')
                    h2 本色酒吧
                    p 广州市天河区兴盛路巴拉巴拉...
                    span.item-note
                       i.icon.ion-location
                       em 2.2km

                a.item.item-avatar
                    img(ng-src='{{vm.barAvatar}}')
